{
  "name": "ledmatrix-webapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "dotenv": "^8.2.0",
    "moment": "^2.28.0",
    "react": "^16.13.1",
    "react-animate-on-scroll": "^2.1.5",
    "react-animations": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-flexbox-grid": "^2.1.2",
    "react-live-clock": "^4.0.5",
    "react-native-dotenv": "^2.4.0",
    "react-scripts": "3.4.3",
    "serve": "^11.3.2",
    "sockjs-client": "^1.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node-env-run server --exec nodemon",
    "dev": "run-p server start"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app"
    ],
    "rules": {
      "no-console": "off",
      "strict": [
        "error",
        "global"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "plugins": [
    [
      "module:react-native-dotenv"
    ]
  ],
  "devDependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-pino-logger": "^5.0.0",
    "node-env-run": "^4.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "pino-colada": "^2.1.0",
    "react-moment": "^0.9.7",
    "styled-components": "^5.2.0"
  }
}
